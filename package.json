{
  "name": "devops-experts-final-project",
  "version": "1.0.0",
  "description": "devops experts final project",
  "homepage": "https://github.com/nirReich/devops-experts-final-project#readme",
  "bugs": {
    "url": "https://github.com/nirReich/devops-experts-final-project/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nirReich/devops-experts-final-project.git"
  },
  "license": "ISC",
  "author": "nir reich",
  "type": "module",
  "main": "none",
  "scripts": {
    "build": "webpack --mode production",
    "dev": "webpack serve --mode development",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "jest --config jest.config.js --coverage",
    "test:integration": "jest --config jest.integration.config.js",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.{js,ts,jsx,tsx}",
    "lint:fix": "eslint src/**/*.{js,ts,jsx,tsx} --fix",
    "format": "prettier --write src/**/*.{js,ts,jsx,tsx,json,css,md}",
    "format:check": "prettier --check src/**/*.{js,ts,jsx,tsx,json,css,md}",
    "type-check": "tsc --noEmit",
    "start": "node dist/server.js",
    "start:dev": "nodemon src/server.js"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.0",
    "prettier": "^3.0.0",
    "typescript": "^5.1.0",
    "webpack": "^5.88.0",
    "webpack-cli": "^5.1.0"
  }
}
